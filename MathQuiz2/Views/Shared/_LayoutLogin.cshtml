    <link rel="shortcut icon" type="image/x-icon" href="../LayoutLogin/img/favicon.ico">

    <!-- CSS here -->
    <link rel="stylesheet" href="../LayoutLogin/css/bootstrap.min.css">
    <link rel="stylesheet" href="../LayoutLogin/css/owl.carousel.min.css">
    <link rel="stylesheet" href="../LayoutLogin/css/slicknav.css">

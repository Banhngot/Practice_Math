<body>
    <!-- ? Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="../LayoutLogin/img/logo/loder.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start-->


    <main class="login-body" data-vide-bg="../LayoutLogin/img/br.jpg">
        @RenderBody()
        <!-- Login Admin -->
        <form class="form-default" action="" method="POST">

            <div class="login-form">
                <!-- logo-login -->
                <div class="logo-login">
                    <a href="index.html"><img src="../LayoutLogin/img/logo/loder.png" alt=""></a>
                </div>
                <h2>Login Here</h2>
                <div class="form-input">
                    <label for="name">Email</label>
                    <input  type="email" name="email" placeholder="Email">
                </div>
                <div class="form-input">
                    <label for="name">Password</label>
                    <input type="password" name="password" placeholder="Password">
                </div>
                <div class="form-input pt-30">
                    <input type="submit" name="submit" value="login">
                </div>

                <!-- Forget Password -->
                <a href="#" class="forget">Forget Password</a>
                <!-- Forget Password -->
                <a href="register.html" class="registration">Registration</a>
            </div>
        </form>
        <!-- /end login form -->
    </main>


    <script src="./../LayoutLogin/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="./../LayoutLogin/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="./../LayoutLogin/js/popper.min.js"></script>
    <script src="./../LayoutLogin/js/bootstrap.min.js"></script>
    <!-- Jquery Mobile Menu -->
    <script src="./../LayoutLogin/js/jquery.slicknav.min.js"></script>

    <!-- Video bg -->
    <script src="./../LayoutLogin/js/jquery.vide.js"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="./../LayoutLogin/js/owl.carousel.min.js"></script>
    <script src="./../LayoutLogin/js/slick.min.js"></script>
    <!-- One Page, Animated-HeadLin -->
    <script src="./../LayoutLogin/js/wow.min.js"></script>
    <script src="./../LayoutLogin/js/animated.headline.js"></script>
    <script src="./../LayoutLogin/js/jquery.magnific-popup.js"></script>

    <!-- Date Picker -->
    <script src="./../LayoutLogin/js/gijgo.min.js"></script>
    <!-- Nice-select, sticky -->
    <script src="./../LayoutLogin/js/jquery.nice-select.min.js"></script>
    <script src="./../LayoutLogin/js/jquery.sticky.js"></script>
    <!-- Progress -->
    <script src="./../LayoutLogin/js/jquery.barfiller.js"></script>

    <!-- counter , waypoint,Hover Direction -->
    <script src="./../LayoutLogin/js/jquery.counterup.min.js"></script>
    <script src="./../LayoutLogin/js/waypoints.min.js"></script>
    <script src="./../LayoutLogin/js/jquery.countdown.min.js"></script>
    <script src="./../LayoutLogin/js/hover-direction-snake.min.js"></script>

    <!-- contact js -->
    <script src="./../LayoutLogin/js/contact.js"></script>
    <script src="./../LayoutLogin/js/jquery.form.js"></script>
    <script src="./../LayoutLogin/js/jquery.validate.min.js"></script>
    <script src="./../LayoutLogin/js/mail-script.js"></script>
